{
  "name": "@cael-aero/gfa-utils",
  "description": "Utilities for interacting with GFA Data",
  "version": "0.7.0",
  "license": "BSD-3-Clause",
  "readme": "README.md",
  "author": "Justin Couch",
  "homepage": "https://github.com/CAELAero/gfa-utils",
  "bugs": {
    "url": "https://github.com/CAELAero/gfa-utils/issues"
  },
  "keywords": [
    "Gliding Federation of Australia",
    "airworthiness",
    "typescript"
  ],
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "engines": {
    "node": ">=10.0.0",
    "npm": ">=6.10.0"
  },
  "files": [
    "lib/**/*",
    "LICENSE.md",
    "tsconfig.json"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/CAELAero/gfa-utils"
  },
  "publishConfig": {
    "tag": "latest",
    "cache": "~/.npm",
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "scripts": {
    "build": "tsc --build",
    "test": "jest --config jest.config.json",
    "coverage": "jest --coverage  && cat ./coverage/lcov.info | coveralls",
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "./node_modules/.bin/eslint src",
    "docs": "typedoc --excludePrivate --excludeProtected src",
    "prepublishOnly": "npm test && npm run lint && npm run docs && git add docs/* && git commit -m \"Auto generated docs\" docs && git push",
    "preversion": "npm run lint",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags"
  },
  "dependencies": {
    "xlsx": "^0.18.0"
  },
  "devDependencies": {
    "@types/jest": "^29.x",
    "@types/node": "^20.x",
    "@typescript-eslint/eslint-plugin": "^6.x",
    "@typescript-eslint/parser": "^6.x",
    "acorn": "^8.x",
    "coveralls": "^3.1",
    "eslint": "^8.x",
    "eslint-config-prettier": "^8.x",
    "eslint-plugin-prettier": "^5.x",
    "eslint-plugin-import": "^2.x",
    "eslint-plugin-prefer-arrow": "^1.2",
    "jest": "^29.0",
    "minimist": ">=1.2.8",
    "prettier": "^3.x",
    "ts-jest": "^29.0",
    "typedoc": "^0.24",
    "typescript": "^5.x"
  }
}
