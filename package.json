{
  "name": "@cael-aero/gfa-utils",
  "description": "Utilities for interacting with GFA Data",
  "version": "0.1.0",
  "license": "BSD-3-Clause",
  "readme": "README.md",
  "author": "Justin Couch",
  "homepage": "https://github.com/CAELAero/gfa-utils",
  "bugs": {
    "url": "https://github.com/CAELAero/gfa-utils/issues"
  },
  "keywords": [
    "Gliding Federation of Australia",
    "airworthiness",
    "typescript"
  ],
  "main": "build/lib/index.js",
  "types": "build/lib/index.d.ts",
  "files": [
    "build/lib/**/*",
    "LICENSE.md",
    "tsconfig.json"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/CAELAero/gfa-utils"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "scripts": {
    "build": "tsc --build",
    "test": "jest --config jestconfig.json",
    "format": "prettier --write \"src/**/*.ts\" \"src/**/*.js\"",
    "lint": "tslint -p tsconfig.json",
    "prepublishOnly": "npm test && npm run lint",
    "preversion": "npm run lint",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags"
  },
  "dependencies": {},
  "devDependencies": {
    "@types/jest": "^25.1.0",
    "@types/node": "^13.7.0",
    "jest": "^25.1.0",
    "prettier": "^1.19.1",
    "ts-jest": "^25.2.0",
    "tslint": "^6.0.0",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^3.5.2",
    "xlsx": "^0.15.5"
  }
}
