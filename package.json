{
  "name": "@cael-aero/gfa-utils",
  "description": "Utilities for interacting with GFA Data",
  "version": "0.2.1",
  "license": "BSD-3-Clause",
  "readme": "README.md",
  "author": "Justin Couch",
  "homepage": "https://github.com/CAELAero/gfa-utils",
  "bugs": {
    "url": "https://github.com/CAELAero/gfa-utils/issues"
  },
  "keywords": [
    "Gliding Federation of Australia",
    "airworthiness",
    "typescript"
  ],
  "main": "build/lib/index.js",
  "types": "build/lib/index.d.ts",
  "engines": {
    "node": ">=10.0.0",
    "npm": ">=6.10.0"
  },
  "files": [
    "build/lib/**/*",
    "LICENSE.md",
    "tsconfig.json"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/CAELAero/gfa-utils"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "scripts": {
    "build": "tsc --build",
    "test": "jest --config jest.config.json",
    "coverage": "jest --coverage  && cat ./coverage/lcov.info | coveralls",
    "format": "prettier --write \"src/**/*.ts\" \"src/**/*.js\"",
    "lint": "./node_modules/.bin/ESLint src",
    "docs": "typedoc --excludePrivate --excludeProtected --out docs src",
    "prepublishOnly": "npm test && npm run lint && npm run docs && git add docs/* && git commit -m \"Auto generated docs\" docs && git push",
    "preversion": "npm run lint",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags"
  },
  "dependencies": {
    "xlsx": "^0.15.6"
  },
  "devDependencies": {
    "@types/jest": "^25.2.3",
    "@types/node": "^13.13.34",
    "acorn": "^7.4.1",
    "coveralls": "^3.1.0",
    "jest": "^25.5.4",
    "minimist": ">=1.2.5",
    "prettier": "^1.19.1",
    "ts-jest": "^25.5.1",
    "eslint": "^7.17.0",
    "@typescript-eslint/parser": "^4.11.1",
    "@typescript-eslint/eslint-plugin": "^4.11.1",
    "eslint-config-prettier": "^7.1.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-prefer-arrow": "^1.2.2",
    "typedoc": "^0.20.8",
    "typedoc-plugin-internal-external": "^2.2.0",
    "typescript": "^3.9.7"
  }
}
