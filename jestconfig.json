{
  "collectCoverage": true,
  "collectCoverageFrom": [
    "src/**/*.ts"
  ],
  "moduleFileExtensions": ["ts", "js", "json", "node"],
  "preset": "ts-jest",
  "testEnvironment": "node",
  "testRegex": "(/test/.*|(\\.|/)(test|spec))\\.(ts?)$",
  "testPathIgnorePatterns": [
    "/node_modules/"
  ],
  "verbose": true
}
